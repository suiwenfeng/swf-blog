{
  "name": "swf-blog",
  "version": "3.0.2",
  "description": "Fully-functional, SEO friendly static site implementation of a blog system built on Ember.",
  "keywords": [
    "ember-addon",
    "ember-ghost",
    "swf-blog",
    "prember-plugin"
  ],
  "repository": "https://github.com/suiwenfeng/swf-blog",
  "license": "MIT",
  "author": {
    "name": "Wenfeng Sui",
    "email": "suiwenfeng@gmail.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "auto-changelog --template changelog.template --unreleased-only --prepend --load-github-issue-data --github-cache-dir .changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "ember-compatibility-tests": "ember try:each",
    "test:node": "mocha node-tests"
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-plugin": "^4.0.3",
    "broccoli-static-site-json": "^4.2.0",
    "broccoli-static-site-map": "^1.0.1",
    "case": "^1.6.1",
    "downsize-cjs": "^0.1.1",
    "ember-auto-import": "^1.3.0",
    "ember-body-class": "^1.1.3",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-showdown": "^4.4.4",
    "ember-cli-string-utils": "^1.1.0",
    "ember-decorators-polyfill": "^1.1.5",
    "ember-get-config": "^0.3.0",
    "ember-meta": "^1.0.0",
    "ember-scroll": "^1.0.2",
    "empress-blueprint-helpers": "^1.2.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.20",
    "resolve": "^1.10.1",
    "walk-sync": "^2.2.0",
    "yaml-front-matter": "^4.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "auto-changelog": "github:mansona/auto-changelog#epic",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-test-helper": "^2.0.0",
    "chai": "^4.2.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.22.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.0.4",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-data": "~3.22.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-responsive-image": "^1.0.0-rc.7",
    "ember-source": "~3.22.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "swf-blog-casper-template": "^3.3.0",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "lint-to-the-future": "^0.4.0-1",
    "lint-to-the-future-eslint": "^0.2.1",
    "loader.js": "^4.7.0",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "prember": "^1.0.2",
    "qunit-dom": "^1.5.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": "ember-meta",
    "before": "prember",
    "demoURL": "https://swf-blog-casper-template.netlify.com"
  }
}
